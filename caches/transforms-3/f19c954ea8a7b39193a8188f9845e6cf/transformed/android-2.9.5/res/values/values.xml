<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#03DAC5</color>
    <color name="colorPrimary">#6200EE</color>
    <color name="colorPrimaryDark">#3700B3</color>
    <string name="app_name">kakaomaps-java-api</string>
    <string name="cadastral_map">cadastral_map</string>
    <string name="normal_map">map</string>
    <string name="overlay_aerial_photo">aerial_photo</string>
    <string name="overlay_bicycle_road">bicycle_road</string>
    <string name="overlay_cctv">cctv</string>
    <string name="overlay_hill_shading">hill_shading</string>
    <string name="overlay_hybrid">hybrid</string>
    <string name="overlay_roadview_line">roadview_line</string>
    <string name="overlay_traffic_info">traffic_info</string>
    <string name="skyview">skyview</string>
    <string name="skyview_3d">skyview_3d</string>
    <style name="LabelBadgeStyle">
        <item name="mapBadgeImage">@null</item>
        <item name="mapBadgeOffsetX">0.5</item>
        <item name="mapBadgeOffsetY">0.0</item>
        <item name="mapBadgeZOrder">0</item>
    </style>
    <style name="LabelStyle">
        <item name="mapIconImage">@null</item>
        <item name="mapIconAnchorX">0.5</item>
        <item name="mapIconAnchorY">1.0</item>
        <item name="mapPadding">0.0</item>
        <item name="mapApplyDpScale">true</item>
    </style>
    <style name="LabelTextStyle">
        <item name="mapTextSize">24</item>
        <item name="mapTextColor">#000000</item>
        <item name="mapTextStrokeWidth">0</item>
        <item name="mapTextStrokeColor">@android:color/transparent</item>
        <item name="mapTextCharacterSpace">0</item>
        <item name="mapTextLineSpace">1.0</item>
        <item name="mapTextAspectRatio">1.0</item>

    </style>
    <style name="PolygonStyle">
        <item name="mapZoomLevel">0</item>
        <item name="mapPolygonColor">#ffffff</item>
        <item name="mapStrokeWidth">0.0</item>
        <item name="mapStrokeColor">@android:color/transparent</item>
    </style>
    <style name="RouteLineStyle">
        <item name="mapLineWidth">0.0</item>
        <item name="mapLineColor">@android:color/transparent</item>
        <item name="mapStrokeWidth">0.0</item>
        <item name="mapStrokeColor">@android:color/transparent</item>
        <item name="mapPatternImage">@null</item>
        <item name="mapSymbolImage">@null</item>
        <item name="mapDistance">10.0</item>
        <item name="mapPinStart">false</item>
        <item name="mapPinEnd">false</item>
    </style>
    <declare-styleable name="AnimationAttr">
        <attr format="integer" name="repeatCount"/>
        <attr format="integer" name="duration"/>
        <attr format="boolean" name="hideShapeAtStop"/>
        <attr format="enum" name="interpolation">
            <enum name="Linear" value="0"/>
            <enum name="CubicIn" value="4"/>
            <enum name="CubicOut" value="5"/>
            <enum name="CubicInOut" value="6"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="CircleWaveAttr">
        <attr format="float" name="startAlpha"/>
        <attr format="float" name="endAlpha"/>
        <attr format="float" name="startRadius"/>
        <attr format="float" name="endRadius"/>
    </declare-styleable>
    <declare-styleable name="LabelBadgeAttr">
        <attr format="reference" name="mapBadgeImage"/>
        <attr format="float" name="mapBadgeOffsetX"/>
        <attr format="float" name="mapBadgeOffsetY"/>
        <attr format="integer" name="mapBadgeZOrder"/>
    </declare-styleable>
    <declare-styleable name="LabelIconAttr">
        <attr format="reference" name="mapIconImage"/>
        <attr format="float" name="mapIconAnchorX"/>
        <attr format="float" name="mapIconAnchorY"/>
        <attr format="float" name="mapPadding"/>
        <attr format="boolean" name="mapApplyDpScale"/>
        <attr format="boolean" name="mapIconEnableEnterTransition"/>
        <attr format="boolean" name="mapIconEnableExitTransition"/>
        <attr format="enum" name="mapIconEnterTransition">
            <enum name="none" value="0"/>
            <enum name="alpha" value="1"/>
            <enum name="scale" value="2"/>
        </attr>
        <attr format="enum" name="mapIconExitTransition">
            <enum name="none" value="0"/>
            <enum name="alpha" value="1"/>
            <enum name="scale" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="LabelTextAttr">
        <attr format="string" name="mapTextFont"/>
        <attr format="integer" name="mapTextSize"/>
        <attr format="reference|color" name="mapTextColor"/>
        <attr format="integer" name="mapTextStrokeWidth"/>
        <attr format="reference|color" name="mapTextStrokeColor"/>
        <attr format="integer" name="mapTextCharacterSpace"/>
        <attr format="float" name="mapTextLineSpace"/>
        <attr format="float" name="mapTextAspectRatio"/>
        <attr format="boolean" name="mapTextEnableEnterTransition"/>
        <attr format="boolean" name="mapTextEnableExitTransition"/>
        <attr format="enum" name="mapTextEnterTransition">
            <enum name="none" value="0"/>
            <enum name="alpha" value="1"/>
            <enum name="scale" value="2"/>
        </attr>
        <attr format="enum" name="mapTextExitTransition">
            <enum name="none" value="0"/>
            <enum name="alpha" value="1"/>
            <enum name="scale" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="MapAttr">
        <attr format="integer" name="mapZoomLevel"/>
        <attr format="enum" name="mapTextGravity">
            <enum name="Left" value="0x01"/>
            <enum name="Right" value="0x02"/>
            <enum name="Top" value="0x04"/>
            <enum name="Bottom" value="0x08"/>
            <enum name="Center" value="0x10"/>
            <enum name="CenterVertical" value="0x20"/>
            <enum name="CenterHorizontal" value="0x40"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="PolygonAttr">
        <attr format="reference|color" name="mapPolygonColor"/>
    </declare-styleable>
    <declare-styleable name="RouteLineAttr">
        <attr format="float" name="mapLineWidth"/>
        <attr format="reference|color" name="mapLineColor"/>
        <attr format="float" name="mapStrokeWidth"/>
        <attr format="reference|color" name="mapStrokeColor"/>
        <attr format="reference" name="mapPatternImage"/>
        <attr format="reference" name="mapSymbolImage"/>
        <attr format="float" name="mapDistance"/>
        <attr format="boolean" name="mapPinStart"/>
        <attr format="boolean" name="mapPinEnd"/>
    </declare-styleable>
</resources>